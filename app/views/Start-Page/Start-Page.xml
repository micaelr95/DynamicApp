<Page loaded="Loaded">
    <StackLayout>
        <Label text="Insert Server URL:" textWrap="true" id="insertLabel"/>
        <TextField text="http://192.168.0.125:2000/" id="urlTextField" />
        <Label text="This can be changed later in the settings page" textWrap="true" id="descriptionlabel"/>
        <Button text="Scan URL QR Code" id="btnScan" onTap="scanQR"/>
        <Button text="Confirm" id="btnConfirm" onTap="confirmURL"/>
    </StackLayout>
</Page>
